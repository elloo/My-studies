// Refactored to work outside Hackerrank environment

function reverseArray(arr) {
	
    var arrLog = [];
    
    for (var i = arr.length - 1; i >= 0; i--){
       arrLog.push(arr[i]);
    }  

    arrLog = arrLog.join(" ");
    arrLog = arrLog.toString();
    console.log(arrLog);
}